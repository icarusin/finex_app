<% provide(:title, @ask.stock.name) %>
<div class="row">
  <aside class="span4">
    <section>
        <h1><%= @ask.stock.name%></h1>
		<h5>Ask placed for <%= @ask.stock.ticker %> for <%= @ask.quantity %> units at <%= @ask.price %> on <%= @ask.created_at %></h5>	
    </section>
  </aside>
</div>
<div class="center">
	<%= link_to "Buy", buy_path, class: "btn btn-large btn-primary" %>
	<%= link_to "Sell", sell_path, class: "btn btn-large btn-primary" %>
</div>